---
{
    "stream": {
        "$id": "technology_listing-page",
        "source": "knowledgeGraph",
        "destination": "pages",
      "filter": {
        "entityTypes": [
          "ce_expertise"
        ],
        "savedFilterIds": [
          "924055047"
        ]
      },
      "fields": [
        "id",
        "uid",
        "name",
        "slug",
        "c_richTextDescription",
        "c_description1",
        "photoGallery",
        "c_relatedMenuCategory.name",
         "c_relatedMenuCategory.slug",
        "c_relatedMenuCategory.photoGallery",
        "c_relatedMenuCategory.c_description1",
        "c_relatedMenuCategory.c_richTextDescription",
        "c_mainRelatedNavber.slug",
"c_mainRelatedNavber.name",
"c_mainRelatedNavber.c_customSubmenuitem.name",
"c_mainRelatedNavber.c_customSubmenuitem.slug",
"c_mainRelatedNavber.c_relatedCategory.c_relatedMenuCategory.name",
"c_mainRelatedNavber.c_relatedCategory.c_relatedMenuCategory.slug"
      ],
      "localization": {
          "locales": ["en"]
      }
    },
    "urlFormat": "{{slug}}"
}
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{name}}</title>

<meta name="viewport" content="width=device-width, initial-scale=1.0">
    {{>head}}
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:200,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/flowbite@1.4.1/dist/flowbite.min.css" />
    <script src="https://unpkg.com/flowbite@1.4.1/dist/flowbite.js"></script>
    <script src="./TW-ELEMENTS-PATH/dist/js/index.min.js"></script>
    
</head>


    <body>
        
  <div class="bg-black ">
   {{>PrototypeMenu}}
    <!-- Image banner code starts here -->


    <!-- Section: Design Block -->
    <section class="mb-10">

      <div class="page-title">
        <div class="centered-container">
          <h1> {{name}} </h1>
          <p>JAVA, Microsoft .Net & Open Source.</p>
        </div>
      </div>

    </section>
    <!-- Section: Design Block -->




    <!-- Image banner code ends here -->


  </div>


        <div class="centered-container">

<div class="bg-black Nav mb-5  mr-3  ml-3 mr-11 rounded">
      <ul class="md:flex items-center bg-black  justify-between text-base text-white pt-4 md:pt-0 py-4 px-5 my-2 rounded">
                      {{#each c_relatedMenuCategory}}
                       <li> <a class="inline-block no-underline text-white hover:text-amber-600 py-1 px-2" href={{slug}}>{{name}}</a></li>
                        
                       {{/each}}
                     
                    </ul>

          </div>        
{{!-- cards --}}

<div  class="grid grid-cols-4 ml-3 items-center justify-center object-center"> 
  {{#each c_relatedMenuCategory}} 
  <a href="{{slug}}">
        <div class="mb-3">
   <img src="{{photoGallery.[0].image.url}}" alt="">
            {{!-- <a class="mb-2 text-2xl font-bold tracking-tight text-gray-00 dark:text-white" href={{slug}}>{{name}}</a> --}}
          
        </div> 
    </a>      
        {{/each}}   
    </div> 
    </div>
 

<div class="w-full site-footer">
    <div class="centered-container">
      {{>footer}}
    </div>
  </div>
</body> 
</html>
