---
{
    "stream": {
        "$id": "startupsDetailed-page",
        "source": "knowledgeGraph",
        "destination": "pages",
      "filter": {
        "entityTypes": [
          "ce_weWorkWith"
        ],
        "savedFilterIds": [
          "917387282"
        ]
      },
      "fields": [
        "id",
        "uid",
        "name",
        "slug",
        "photoGallery",
        "c_description1",
        "c_richTextDescription",
        "c_related01.name",
        "c_related01.c_description1",
        "c_relatedCategory.name",
        "c_relatedCategory.photoGallery",
        "c_relatedCategory.c_description"
      ],
      "localization": {
          "locales": ["en"]
      }
    },
    "urlFormat": "{{slug}}"
}
---

