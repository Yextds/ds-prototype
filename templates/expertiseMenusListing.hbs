---
{
"stream": {
"$id": "expertisePage-onClick-open",
"source": "knowledgeGraph",
"destination": "pages",
"filter": {
"entityTypes": [
"ce_expertisePage"
]
},
"fields": [
"id",
"uid",
"name",
"slug",
"c_dsibHeader",
"c_dsibParagraph",
"c_description",
"c_richTextDescription",
"c_relatedCategory.name",
"c_relatedCategory.slug",
"c_relatedCategory.photoGallery",
"c_relatedCategory.c_richTextDescription",
"c_relatedCategory.c_description1",
"c_relatedMenuCategory.name",
"c_relatedMenuCategory.slug",
"c_relatedMenuCategory.photoGallery",
"c_mainRelatedNavber.slug",
"c_mainRelatedNavber.name",
"c_mainRelatedNavber.c_customSubmenuitem.name",
"c_mainRelatedNavber.c_customSubmenuitem.slug",
"c_mainRelatedNavber.c_customSubmenuitem.c_relatedMenuCategory.name",
"c_mainRelatedNavber.c_customSubmenuitem.c_relatedMenuCategory.slug"
],
"localization": {
"locales": ["en"]
}
},
"urlFormat": "expertise.html"
}
---

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>{{name}}</title>

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {{>head}}
  <link href="https://fonts.googleapis.com/css?family=Work+Sans:200,400&display=swap" rel="stylesheet">

</head>

<body>
  {{>PrototypeMenu}}

  <!-- Image banner code starts here -->
  <div>

    <!-- Section: Design Block -->
    <section class="mb-40">
      <div class="page-title">
        <div class="centered-container">
          <h1> {{c_dsibHeader}} </h1>
          <p>{{c_dsibParagraph}}</p>
        </div>
      </div>


    </section>
    <!-- Section: Design Block -->

  </div>

  <!-- Image banner code ends here -->

  <!-- Main Content area starts here -->

  <!-- Bannner code starts here -->

  <div class="reverse-cl">
    {{#each c_relatedCategory}}
    <div class="w-10/12 abc md:w-full lg:w-full px-12 md:px-0 mr-auto ml-auto -mt-0">
      <div class="relative flex  items-center flex-col md:flex-row min-w-0 break-words w-full mb-6 shadow-lg bg-[#f5821f]">
        <div class=" image-area flex-initial w-1/2"> <img alt="..." src="{{photoGallery.[0].image.url}}"> </div>

        <div class="content-area flex-initial w-1/2">

          <blockquote class="relative p-8 mb-4">
            {{!-- <svg preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 583 95"
              class="absolute left-0 w-full block" style="height:95px;top:-94px">
              <polygon points="-30,95 583,95 583,65" class="text-pink-600 fill-current"></polygon>
            </svg> --}}
            <h2 class="text-xl font-bold text-white">{{name}}</h2>
            <p class="text-md font-light mt-2 text-white">{{c_description1}}</p>
          </blockquote>
        </div>
      </div>
      </div>
      {{/each}}
    </div>

    <!-- Banner code ends here -->

    <!-- Paragraph code starts here  -->
    <div class="centered-container pb-14">

    <div class="p-12 text-center  text-gray-700">
      <h2 class="font-semibold text-4xl mb-4">{{c_richTextDescription}}</h2>
      <h5 class="font-semibold text-xl mb-6">{{c_description}}</h5>
    </div>

    <!-- Paragraph code ends here -->

    <!-- Whom we work with code starts here -->

    <div>
      <div class="grid grid-cols-3 gap-4">
        {{#each c_relatedMenuCategory}}
        <div class="mb-4">
          <div class="relative overflow-hidden bg-no-repeat bg-cover w-full">
            <img alt="..." class="max-w-full w-full rounded-lg shadow-lg  aspect-video" src="{{photoGallery.[0].image.url}}" width="634"
              height="951">
            <div
              class="absolute top-0 right-0 bottom-0 rounded-lg left-0 w-full h-full overflow-hidden bg-fixed opacity-0 hover:opacity-50 transition duration-300 ease-in-out bg-red-700">
            </div>
          </div>
          <div class=" relative -mt-4 text-center">
            <a href="{{slug}}" class="inline-block   px-6 py-2.5 bg-black text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-gray-300 hover:shadow-lg focus:bg-gray-300 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-gray-400 active:shadow-lg transition duration-150 ease-in-out">
              
                {{name}}
            </a>
          </div>

        </div>
        {{/each}}
      </div>

    </div>

</div>
    <!-- Whom we work with code ends here -->



    <!-- Main Content area ends here -->


    <!-- Footer code starts here -->
    <div class="w-full site-footer">
      <div class="centered-container">
        {{>footer}}
      </div>
    </div>
    <!-- Footer code ends here -->

</body>

</html>