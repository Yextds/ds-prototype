            addressCat.forEach((m, e) => {

              console.log(m.name);
              let varName = m.name;

              console.log(m);

              let cty = m.address.city;
              let lineOne = m.address.line1;
              let lineTwo = m.address.line2;
              let pC = m.address.postalCode;

              let varRegion = m.address.region;
              let mP = m.mobilePhone;

               console.log(mP);
               if (pC === undefined) {
                  addressHtml += '<li>' + '<strong>' + varName + '</strong>' + ',&nbsp;' + lineOne + '<strong>' + '<a href=tel:' + mP + '> Tel:' + mP + '</a></strong></li>';
               }
              
               else  if (varRegion === undefined) {
                  addressHtml += '<li>' + '<strong>' + varName + '</strong>' + ',&nbsp;' + lineOne + ',' + pC + '<strong>' + '<a href=tel:' + mP + '> Tel:' + mP + '</a></strong></li>';
